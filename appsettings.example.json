{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"ConnectionStrings": {
		"DefaultConnection": "Host=localhost;Port=5432;Database=your-database;Username=your-username;Password=your-password"
	},
	"Jwt": {
		"Key": "your-jwt-key",
		"Issuer": "your-jwt-issuer",
		"Audience": "yout-jwt-audience",
		"Subject": "your-jwt-subject"
	},
	"InitialUserData": {
		"INITIAL_USER_USERNAME": "your-initial-username",
		"INITIAL_USER_EMAIL": "your-initial-email",
		"INITIAL_USER_PASSWORD": "your-initial-password"
	},
	"AllowedHosts": "*"
}
